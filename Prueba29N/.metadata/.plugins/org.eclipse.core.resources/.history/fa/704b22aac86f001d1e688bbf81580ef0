def generar_usuario(nombre, apellido1, apellido2, dni):
    usuario=""
    a=0
    b=-1
    c = 0
    d = -2
    while a < 3:
        usuario+=nombre[a]
        a+=1
    while b > -4:
        usuario+=apellido1[b]
        b-=1
    while c < 3:
        usuario+=apellido2[c]
        c+=1
    while d > -6:
        usuario+=dni[d]
        d-=1     
    return usuario

print(generar_usuario("Ivan", "Sanchez", "Sanchez", "74568934A"))